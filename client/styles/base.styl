$desktopIncrement = 64px
$desktopIncrementHalf = $desktopIncrement * 0.5
$desktopIncrementQuarter = $desktopIncrement * 0.25

$fullBlack = rgba(0, 0, 0, 1)
$darkBlack = rgba(0, 0, 0, 0.87)
$lightBlack = rgba(0, 0, 0, 0.54)

$fullWhite = rgba(255, 255, 255, 1)
$darkWhite = rgba(255, 255, 255, 0.87)
$lightWhite = rgba(255, 255, 255, 0.54)

$shadow0 = 0 -1px 0 #e5e5e5, 0 0 2px rgba(0, 0, 0, 0.12), 0 2px 4px rgba(0, 0, 0, 0.24)
$shadow1 = 0 2px 2px 0 rgba(0, 0, 0, 0.14), 0 3px 1px -2px rgba(0, 0, 0, 0.2), 0 1px 5px 0 rgba(0, 0, 0, 0.12)
$shadow2 = 0 3px 4px 0 rgba(0, 0, 0, 0.14), 0 3px 3px -2px rgba(0, 0, 0, 0.2), 0 1px 8px 0 rgba(0, 0, 0, 0.12)
$shadow3 = 0 4px 5px 0 rgba(0, 0, 0, 0.14), 0 1px 10px 0 rgba(0, 0, 0, 0.12), 0 2px 4px -1px rgba(0, 0, 0, 0.2)

$transitionAllCubic = all 450ms cubic-bezier(0.23, 1, 0.32, 1) 0ms

@import url(https://fonts.googleapis.com/css?family=Lato:400,300,700);
@import "mui_colours"

body
	font-family 'Lato'
